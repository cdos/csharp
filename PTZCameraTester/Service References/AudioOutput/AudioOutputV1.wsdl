<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="urn:schemas-pelco-com:service:AudioOutput:1" targetNamespace="urn:schemas-pelco-com:service:AudioOutput:1" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
  <wsdl:documentation>
    	$Id: AudioOutputV1.wsdl 474 2010-12-22 19:25:11Z blee $
	</wsdl:documentation>
  <wsdl:types>
    <xsd:schema targetNamespace="urn:schemas-pelco-com:service:AudioOutput:1" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
      <xsd:complexType name="AudioQuality">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						This represents audio quality settings.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="framerate" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The number of frames being transferred per second.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="bitrate" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The number of bits being transferred per second.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="format" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The audio stream's format.
								
								Valid values are the following:
								<ul><li>G764</li><li>G711</li></ul></xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="samplesize" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The size of the sample.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="samplerate" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The number of samples per second.
								
								Valid values are the following:
								<ul><li>For a high rate use: 32 Kbps</li><li>For a medium rate use: 16 Kbps</li><li>For a low rate use: 8 Kbps</li></ul></xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="volume" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The sound level.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="StreamSession">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="sourceURI" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The URI of the original source of the audio data.
								
								e.g. uuid:EnduraEncoder-CU6350-5/urn:pelco-com:serviceId:AudioOutput-1
 								e.g. uuid:ca522d4a-d26f-446a-8e7e-ccdb6127fd2d/urn:pelco-com:serviceId:AudioOutput-16
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="outputURI" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The URI to which the audio will be streamed.
								
								e.g. uuid:EnduraDecoder-CU6350-5/urn:pelco-com:serviceId:StreamControl-1
    							e.g. uuid:713119bb-401c-4c3e-bd1f-3443a45e28bc/urn:schemas-pelco-com:service:AudioOutput:1
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="controlURI" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The URI of the urn:schemas-pelco-com:StreamControl 
								service to be used to control the indicated stream.
								
								e.g. uuid:EnduraDecoder-CU6350-5/urn:pelco-com:serviceId:StreamControl-1
    							e.g. uuid:713119bb-401c-4c3e-bd1f-3443a45e28bc/urn:schemas-pelco-com:service:AudioOutput:1
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="transportURL" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The URL to which the selected video data will be transmitted. 
								By default, audio streams are multicast unless this value is 
								filled. The device port is optional; the server response will 
								include this if it is not specified in the request.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="streamState" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The current transmission state of the stream. 
								<ul><li>Play</li><li>Reverse</li><li>Pause</li><li>Seek</li></ul></xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="sessionId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The unique identifier to be used in addressing 
								the urn:schemas-pelco-com:StreamControl service 
								indicated by controlURI. It is in the UUID format.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="startTime" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The start time of the interval being played back. 
								The format of start time is dateTime.tz. 
								ex 1988-04-07T18:39:09-08:00
							    NOW is also valid to denote a live stream.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="endTime" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The end time of the interval being played back.
								The format of the end time is dateTime.tz. ex 1988-04-07T18:39:09-08:00
								INFINITE is also valid to denote a live stream.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="clients" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The number of clients currently associated to this session.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="refreshInterval" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The interval in seconds to be used in the RefreshSession 
								action in the StreamControl service 
								indicated by controlURI. If the camera does not receive 
								any calls to the RefreshSession action within the specified 
								interval, the audio stream session identified by sessionId 
								will be disconnected. Units are in seconds. The minimum 
								value is 30 and the maximum value is 900.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="transportUrlUseSourceAddress" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								If specified and non-zero, when requesting a 
								unicast stream, tells the device to replace the 
								ip address specified in transportURL with the 
								originating ip address of the request
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="StreamParameters">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="displayName" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								This denotes what identifying text to display onscreen
								with the stream.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="eventType" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								Recording or event type that caused the stream 
								to be recorded. For example: Scheduled, Motion, 
								Alarm, Manual.
								
								This is primarily only returned by NVR queries.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="preAlarmDurationInManualRecord" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								This specifies the amount of time, in seconds, 
								prior to the current time that the recording 
								should begin. Typically this is used when  
								manual recording is initiated. This has little 
								to do with alarms.
								
								30 (seconds) is common value. 
								
								This feature is based on the premise that the 
								recording device has cached some amount of 
								video prior to the current time.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="streamSession" type="tns:StreamSession" />
          <xsd:element name="audioQuality" type="tns:AudioQuality" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="EVENT-URN">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						This contains available event types:
						<ul><li>
								AlarmRecord - represents important triggered 
								violations
							</li><li>
								MotionRecord - represents a triggered motion 
								detection related event
							</li><li>
								TimedRecord - represents an event triggered by
								time duration
							</li><li>
								MarkRecord - represents an event triggered by a
								user initiated marking of a specific frame
							</li><li>
								AllRecord - represents all events
							</li></ul></xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="AlarmRecord" />
          <xsd:enumeration value="MotionRecord" />
          <xsd:enumeration value="TimedRecord" />
          <xsd:enumeration value="MarkRecord" />
          <xsd:enumeration value="AllRecord" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="StreamQuery">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						This contains criteria used for filtering audio stream 
						query results.

						Some important points regarding the form of the search 
						criteria (StreamQuery data element):
						<ul><li>
								The search criteria only need contain those 
								elements needed to filter the returned catalog, 
								or used to specify which elements should be 
								included in the response.
							</li><li>
								Those elements that should be included in the 
								response should contain a "select" attribute. 
								If a "select" attribute is associated with the 
								root streamQuery element, all sub-elements 
								will be returned.
							</li><li>
								Those elements whose existence is required 
								should contain a required attribute. As with 
								the select attribute, if a "required" attribute 
								is associated with the root streamQuery 
								element, then all sub-elements are required. 
							</li></ul><div class="notice">
							A direct transformation exists between the search 
							criteria and an SQL statement of the form 
							SELECT [syseng:Fields] FROM [syseng:Table] 
							WHERE [syseng:Conditions] where [syseng:Fields] 
							includes those elements with an associated 
							"select" attribute (or is considered to be * if 
							the streamQuery element has a "select" attribute), 
							[syseng:Table] is empty, and [syseng:Conditions] is 
							the logical and of the included search criteria.
						</div></xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" name="sourceURI" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The URL from which the search results may be 
								downloaded. 
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="sourceId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The unique identifier of the AudioInput service 
								that was original source of the video data.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="outputUDN" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The unique device name where this service will 
								direct its output.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="outputId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The unique identifier of the instance where this 
								service will direct its output.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="eventUDN" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The unique device name where the service will 
								direct its events.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="eventId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The unique identifier of a specific event.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="eventURN" type="tns:EVENT-URN" />
          <xsd:element minOccurs="0" name="serviceId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The service's unique identifier.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="formats" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								A list of acceptable formats to be used in the search criteria. 
								
								Valid values are the following:
								<ul><li>G764</li><li>G711</li></ul></xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="selected" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The audio stream related attributes to return. 
								(Akin to SQL's SELECT statement.)
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="minBitrate" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								A minimum bit-rate to be used in the search criteria, in bps
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="maxBitrate" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								A maximum bit-rate to be used in the search criteria, in bps
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="startTime" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The start time of the interval being played back. 
								The format of start time is dateTime.tz. 
								ex 1988-04-07T18:39:09-08:00
							    NOW is also valid to denote a live stream.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="endTime" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The end time of the interval being played back.
								The format of the end time is dateTime.tz. ex 1988-04-07T18:39:09-08:00
								INFINITE is also valid to denote a live stream.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="pageHeader" type="tns:PageHeader" />
          <xsd:element minOccurs="0" name="deletionPriority" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The deletion order designation during DVR / NVR / NSM
								garbage collection.
								
								This is restricted to the following values (from
								lowest to highest):
								<ul><li>0 for DISCARD</li><li>1 for LOW</li><li>2 for MEDIUM</li><li>3 for HIGH</li><li>4 for LOCK - this is the highest priority</li><li>5 for UNDEFINED</li></ul></xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Entries">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A list of StreamParameters.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="streamParameters" type="tns:StreamParameters" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="QueryId">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A query's unique identifier.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:string">
            <xsd:attribute name="ttl" type="xsd:unsignedInt">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									The query's time to live, before the results 
									of the query are no longer stored.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="PageHeader">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="queryId" type="tns:QueryId">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								A query's unique identifier.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="nextEntry" type="xsd:unsignedInt">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								An index bookmarking the next entry that signifies
								the next page of results.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="totalEntries" type="xsd:unsignedInt">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The total number of entries that the device can
								support.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="maxEntries" type="xsd:unsignedInt">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The maximum number of results to return.
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="resultURI" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								The URL from which the search results may be 
								downloaded. This may be used in conjuntion with 
								entry paging to improve performance. 
							</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="StreamCatalog">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						This contains a list of stream related data as well as 
						paging result information.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" name="pageHeader" type="tns:PageHeader" />
          <xsd:element minOccurs="0" name="entries" type="tns:Entries">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">
								An Entries data type.
								
								<div>
									.NET cannot handle a maxOccurs of unbounded for this 
									element.
								</div></xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="Query">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A request to the Query action.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamQuery" type="tns:StreamQuery">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									Represents the query. A StreamQuery data type.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="asyncId" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									This is only relevant to Endura and is 
									related to multi-cast messages from Pelco
									GUI client software.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="QueryResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A response to a Query request.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamCatalog" type="tns:StreamCatalog" />
            <xsd:element minOccurs="0" name="asyncId" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									This is only relevant to Endura and is 
									related to multi-cast messages from Pelco
									GUI client software.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Connect">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A request to the Connect action.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamParameters" type="tns:StreamParameters" />
            <xsd:element minOccurs="0" name="asyncId" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									This is only relevant to Endura and is 
									related to multi-cast messages from Pelco
									GUI client software.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ConnectResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A response to a Connect request.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamParameters" type="tns:StreamParameters" />
            <xsd:element minOccurs="0" name="asyncId" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									This is only relevant to Endura and is 
									related to multi-cast messages from Pelco
									GUI client software.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Disconnect">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A request to the Disconnect action.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="sessionId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									An audio stream session's identifier.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="asyncId" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									This is only relevant to Endura and is 
									related to multi-cast messages from Pelco
									GUI client software.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="DisconnectResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A response to a Disconnect request.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="asyncId" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									This is only relevant to Endura and is 
									related to multi-cast messages from Pelco
									GUI client software.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="RefreshSession">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A request to the RefreshSession action.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="sessionId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									An audio stream session's identifier.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="refreshInterval" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
									This is only relevant to Endura and is 
									related to multi-cast messages from Pelco
									GUI client software.
								</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="RefreshSessionResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
						A response to a RefreshSession request.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType />
      </xsd:element>
