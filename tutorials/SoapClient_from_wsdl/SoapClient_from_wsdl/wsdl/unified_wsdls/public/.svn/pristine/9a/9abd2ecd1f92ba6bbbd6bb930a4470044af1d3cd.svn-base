<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="wsdl-viewer.xsl"?>

<wsdl:definitions
  xmlns:tns="urn:schemas-pelco-com:service:StreamConfiguration:1"
  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  targetNamespace="urn:schemas-pelco-com:service:StreamConfiguration:1">
  <wsdl:documentation> $Id: StreamConfigurationV1.wsdl 433 2010-11-23 01:18:25Z
    $ </wsdl:documentation>

  <wsdl:service name="StreamConfiguration">
    <wsdl:documentation><p>This service is used to configure the properties of
        streams available from a device.</p><p>In order to make this service
        flexible and applicable to different devices, configuration settings are
        not explicitly specified. Instead, this service will return the
        available settings and their possible values. To specify a device's
        configuration, you need to define one setting at a time. This iterative
        approach is required because each setting choice can affect the possible
        values and availability for other settings.</p></wsdl:documentation>
    <wsdl:port binding="tns:StreamConfiguration" name="StreamConfiguration">
      <soap:address
        location="http://localhost:49152/control/StreamConfiguration-1"/>
    </wsdl:port>
  </wsdl:service>

  <wsdl:types>
    <xsd:schema
      targetNamespace="urn:schemas-pelco-com:service:StreamConfiguration:1">
      <xsd:complexType name="StreamID">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The stream's unique identifier. Each
            device can output a certain number of streams. These streams are
            identified by an integer value: streamID.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="type" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">The type of
                stream.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="id" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">An integer value assigned by
                order of priority. The first stream is assigned the id 0.
                Subsequent streams are assigned consecutive values (1, 2, ...)
                in order of priority. 0 is the "primary" stream, 1 is the
                "secondary" stream, and so on.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ResourceUtilization">
        <xsd:all>
          <xsd:element name="resource" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">The name of a
                resource.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="utilizationPct" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">The percentage of utilization of
                the previously mentioned resource.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:all>
      </xsd:complexType>
      <xsd:complexType name="Resolution">
        <!-- frame size? -->
        <xsd:annotation>
          <xsd:documentation xml:lang="en">The viewable size of a
            stream.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="width" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">The horizontal size of the
                image.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="height" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">The vertical size of the
                image.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ResolutionEnumeration">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A list of Resolution data
            elements.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="value" type="tns:Resolution" minOccurs="0"
            maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A list of Resolution data
                elements.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Setting">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This type is essentially a name-value
            pair that can contain different types of values. This is used in
            conjunction with SettingOptions.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="name" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">The name of the
                setting.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="decimalValue" type="xsd:decimal">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A decimal value used for
                  specifying setting values such as
                  framerate.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="stringValue" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A string value used for
                  specifying setting values such as
                  encoding.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="resolutionValue" type="tns:Resolution">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A Resolution data element used
                  for specifying resolution values.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="intValue" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">An integer value used for
                  specifying setting values such as gop, bitrate, and
                  quality.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="booleanValue" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A boolean value used for
                  flags.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="StringEnumeration">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A list of string
            values.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="value" type="xsd:string" minOccurs="0"
            maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A list of string
                values.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="FreeFormString">
        <xsd:sequence>
          <xsd:element name="maxLength" type="xsd:int"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DecimalEnumeration">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A list of decimal
            values.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="value" type="xsd:decimal" minOccurs="0"
            maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A list of decimal
                values.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="IntRange">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">A range of integer
            values.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="minValue" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">The smallest allowed integer
                value.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="maxValue" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">The largest allowed integer
                value.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="default" type="xsd:int" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">(optional) the default integer
                value.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="BooleanOption">
        <xsd:sequence>
          <xsd:element name="default" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">(optional) the default boolean
                value.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="SettingOptions">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">This describes a configuration option
            (its name) and possible acceptable values for this configuration
            option.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="name" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">(optional) the name of the
                setting whose possible value(s) is being described
                here.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="stringEnumeration" type="tns:StringEnumeration">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A StringEnumeration data
                  element which typically represents possible encoding
                  values</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="freeFormString" type="tns:FreeFormString">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A FreeFormString data
                  element</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="decimalEnumeration" type="tns:DecimalEnumeration">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A DecimalEnumeration data
                  element which typically represents possible frame rate
                  values</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="resolutionEnumeration"
              type="tns:ResolutionEnumeration">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A ResolutionEnumeration data
                  elements which typically represents possible resolution values
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="intRange" type="tns:IntRange">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A IntRange data elements which
                  typically represents possible gop, bitrate, and quality
                  values</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="boolean" type="tns:BooleanOption">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A boolean
                  value</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="StreamSettings">
        <xsd:sequence>
          <xsd:element name="setting" type="tns:Setting" minOccurs="0"
            maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A list of Setting data
                elements.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="QueryResult">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Encapsulates the results of a query
            for streams.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="setting" type="tns:Setting" minOccurs="0"
            maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A list of Setting data
                elements</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="settingOptions" type="tns:SettingOptions"
            minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A list of SettingOptions data
                elements</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="StreamConfigurationInfo">
        <xsd:sequence>
          <xsd:element name="stream" type="tns:StreamID">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A StreamID data
                element</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="result" type="tns:QueryResult">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A QueryResult data
                element</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ConfigurationInfo">
        <xsd:sequence>
          <xsd:element name="resourceUtilization" type="tns:ResourceUtilization"
            minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A ResourceUtilization data
                element</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="streamInfo" type="tns:StreamConfigurationInfo"
            minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A StreamConfigurationInfo data
                element</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="StreamIDSettings">
        <xsd:annotation>
          <xsd:documentation xml:lang="en"/>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="stream" type="tns:StreamID">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A StreamID data
                element</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="settings" type="tns:StreamSettings">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A StreamSettings data
                element</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="StreamIDSettingsList">
        <xsd:sequence>
          <xsd:element name="streamIDSettings" type="tns:StreamIDSettings"
            minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A list of StreamIDSettings data
                elements</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Preset">
        <xsd:all>
          <xsd:element name="descriptiveName" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A preset's friendly
                name.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="valid" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">True if valid, false
                otherwise.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="streamSettings" type="tns:StreamIDSettingsList">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A StreamIDSettingsList data
                element.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:all>
      </xsd:complexType>
      <xsd:complexType name="PresetList">
        <xsd:sequence>
          <xsd:element name="preset" type="tns:Preset" minOccurs="0"
            maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation xml:lang="en">A Preset data
                element.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <!--GetStreamCount-->
      <xsd:element name="GetStreamCount">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Returns the number of distinct stream configurations supported by the device.
            This is the number of distinct stream configurations that could be returned by multiple calls to
            the GetCurrentConfiguration action.
            Therefore, the value returned by GetStreamCount is the number of
            distinct stream configurations, regardless of the current configuration.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType/>
      </xsd:element>
      <xsd:element name="GetStreamCountResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a response from the
            GetStreamCount action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamCount" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  Number of distinct stream configurations.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <!--GetCurrentConfiguration-->
      <xsd:element name="GetCurrentConfiguration">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a request to the
            GetCurrentConfiguration action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="stream" type="tns:StreamID">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A StreamID data
                  element.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="GetCurrentConfigurationResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a response from the
            GetCurrentConfiguration action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="result" type="tns:QueryResult">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A QueryResult data
                  element</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <!--QueryOptions-->
      <xsd:element name="QueryOptions">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a request to the
            QueryOptions action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="stream" type="tns:StreamID">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A StreamID data
                  element</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="filter" type="tns:StreamSettings">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A StreamSettings data
                  element</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="QueryOptionsResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a response from the
            QueryOptions action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="result" type="tns:QueryResult">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A QueryResult data
                  element</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <!--SaveSettings-->
      <xsd:element name="SaveSettings">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a request to the
            SaveSettings action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="stream" type="tns:StreamID">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A StreamID data
                  element.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="settings" type="tns:StreamSettings">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A list of StreamSettings data
                  elements. <div class="notice">This must contian the full
                    configuration, all of the settings and their respective
                    values; even for settings that are not being
                  changed.</div></xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="SaveSettingsResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a response from the
            SaveSettings action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType/>
      </xsd:element>
      <!--GetAllSettings-->
      <xsd:element name="GetAllSettings">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a request to the
            GetAllSettings action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamType" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">Describes the stream (audio,
                  video, and so on)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="GetAllSettingsResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a response from the
            GetAllSettings action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamSettingsList"
              type="tns:StreamIDSettingsList">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A StreamIDSettingsList data
                  element.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <!--GetAllCurrentConfiguration-->
      <xsd:element name="GetAllCurrentConfiguration">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a request to the
            GetAllCurrentConfiguration action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamType" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">Describes the stream (audio,
                  video, etc.)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="GetAllCurrentConfigurationResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a response from the
            GetAllCurrentConfiguration action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="result" type="tns:ConfigurationInfo">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A ConfigurationInfo data
                  element.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <!--QueryAllOptions-->
      <xsd:element name="QueryAllOptions">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a request to the
            QueryAllOptions action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamType" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">Describes the stream (audio,
                  video, etc.)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="streamSettingsList"
              type="tns:StreamIDSettingsList">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A StreamIDSettingsList data
                  element.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="QueryAllOptionsResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a response from the
            QueryAllOptions action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="result" type="tns:ConfigurationInfo">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A ConfigurationInfo data
                  element.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <!--SaveAllSettings-->
      <xsd:element name="SaveAllSettings">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a request to the
            SaveAllSettings action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamType" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">Describes the stream (audio,
                  video, etc.)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="streamSettingsList"
              type="tns:StreamIDSettingsList">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">A StreamIDSettingsList data
                  element.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="SaveAllSettingsResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a response from the
            SaveAllSettings action.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType/>
      </xsd:element>
      <!--ApplyDynamicSettings Definition-->
      <xsd:element name="ApplyDynamicSettings">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a request to temporarily
            and dynamically apply one or more settings without restarting the
            stream.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="streamType" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">describes the stream (audio,
                  video, etc.)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="streamSettingsList"
              type="tns:StreamIDSettingsList">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">a StreamIDSettingsList data
                  element</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ApplyDynamicSettingsResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">Represents a response from the
            ApplyDynamicSettings action</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType/>
      </xsd:element>

      <!--GetPresets-->
      <xsd:element name="GetPresets">
        <xsd:annotation>
