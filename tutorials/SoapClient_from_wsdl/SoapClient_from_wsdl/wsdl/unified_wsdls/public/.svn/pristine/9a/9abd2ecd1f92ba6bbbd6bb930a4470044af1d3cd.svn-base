        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <!--ApplyDynamicSettings Operation-->
    <wsdl:operation name="ApplyDynamicSettings">
      <wsdl:documentation>This operation will set one or more configuration
        parameters, e.g. the bit rate and frame rate, temporarily and
        dynamically without recalculating resources. This new operation should
        be a subset of the already identified maximum configurations in order to
        reserve/allocate enough space without resetting the stream. It will be
        tied specifically to single video stream.</wsdl:documentation>
      <soap:operation
        soapAction="urn:schemas-pelco-com:service:StreamConfiguration:1#ApplyDynamicSettings"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="GetPresets">
      <soap:operation
        soapAction="urn:schemas-pelco-com:service:StreamConfiguration:1#GetPresets"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="GetCurrentPresetName">
      <soap:operation
        soapAction="urn:schemas-pelco-com:service:StreamConfiguration:1#GetCurrentPresetName"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>

